{
  "name": "anchors-away-js",
  "version": "0.1.1",
  "private": false,
  "main": "lib/anchors-away.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-web": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --dest lib --name anchors-away ./src/lib/anchors-away.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
